- name: Obtenir les sw en  version 15.1
  gather_facts: false
  hosts: switchs
  tasks:
    - name: collect ouput show commands
      ios_command:
        commands:
          - show  version
        wait_for:
          - result[0] contains 15.2

      #register: all_conf

